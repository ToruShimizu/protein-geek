{
  "extends": [
    "./tsconfig.json",
    "next/core-web-vitals",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "prettier"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  },
  "includes": ["src/**/*.ts", "src/**/*.tsx", ".eslintrc.json"],
  "exclude": ["node_modules", "dist"]
}
